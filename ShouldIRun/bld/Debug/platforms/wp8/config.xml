<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.ShouldIRun" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>ShouldIRun</name>
    <description>
    A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.
  </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    Apache Cordova Team
  </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="false" />
    <vs:features>
        <vs:feature>org.apache.cordova.geolocation@0.3.7</vs:feature>
    </vs:features>
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget platformName="windows8" />
    </vs:platformSpecificValues>
</widget>
